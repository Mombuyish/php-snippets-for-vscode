{
    "name": "php-snippets-for-vscode",
    "displayName": "PHP snippets for vscode",
    "description": "This extension will help you faster to make common using snippets.",
    "version": "1.1.0",
    "publisher": "Yish",
    "engines": {
        "vscode": "^1.27.0"
    },
    "icon": "images/elephant.png",
    "galleryBanner": {
        "color": "#DD4C4F",
        "theme": "light"
    },
    "keywords": [
        "PHPStorm",
        "Live Templates",
        "PHP",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Mombuyish/php-snippets-for-vscode.git"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
